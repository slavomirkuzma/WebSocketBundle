{
	"name" : "gos/web-socket-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Web Socket Bundle",
	"keywords" : [
		"Web Socket Bundle",
		"Websocket",
		"WAMP",
		"IO",
		"Ratchet"
	],
	"homepage" : "https://github.com/GeniusesOfSymfony/WebSocketBundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Jeremy Dare",
			"email" : "jeremy.d.dare@gmail.com"
		}, {
			"name" : "Johann Saunier",
			"email" : "johann_27@hotmail.fr"
		}
	],
	"require" : {
		"php" : ">=5.4",
		"symfony/framework-bundle" : "^3.0.1",
		"gos/pubsub-router-bundle" : "dev-sf3",
		"gos/websocket-client" : "0.1.2",
		"gos/ratchet-stack" : "~0.1",
		"gos/pnctl-event-loop-emitter" : "0.1.5",
		"gos/ratchet" : "~0.3.5"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^4.8"
	},
	"replace" : {
		"cboden/ratchet" : "~0.3.0"
	},
	"autoload" : {
		"psr-4" : {
			"Gos\\Bundle\\WebSocketBundle\\" : ""
		}
	},
	"suggest" : {
		"ext-libevent" : ">=0.0.5",
		"ext-libev" : "*",
		"ext-zmq" : "*",
		"ext-amqp" : "*",
		"symfony/proxy-manager-bridge" : "~2.3",
		"ocramius/proxy-manager" : "~1.0"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.7.x-dev"
		}
	}
}